<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cc.mrbird.febs.policy.mapper.ThemeMapper">
    <select id="getThemeList"  resultType="Theme">
        SELECT
        p.id,
        p.firstname,
        p.secondname,
        p.thirdname
        FROM
        theme p
    </select>
</mapper>
